                                            <FareDemandFactorRef ref="ENT:FareDemandFactor:SchoolDemandYear2020-2021-Bergen" version="1"/>
                                        </CustomerPurchaseParameterAssignment>
                                    </includes>
                                </CustomerPurchaseParameterAssignment>

