                                    <TypeOfAccessRightAssignmentRef ref="ENT:TypeOfAccessRightAssignment:ValidityRule" version="1"/>
                                    <ValidableElementRef ref="ENT:ValidableElement:SingleTrip" version="1"/>
                                    <limitations>
