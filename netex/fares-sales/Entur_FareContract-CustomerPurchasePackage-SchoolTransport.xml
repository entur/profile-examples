                                <CustomerPurchaseParameterAssignment version="1" id="ENT:CustomerPurchaseParameterAssignment:ForbrukGjennomSkoleår" order="1">
                                    <ValidableElementRef ref="ENT:ValidableElement:SingleTrip" version="1"/>
