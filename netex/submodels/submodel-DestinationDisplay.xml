<?xml version="1.0" encoding="UTF-8"?>
<!--
DestinationDisplay is used to specify text that is advertized to public, both on the screens on vehicle sides and on some other media
-->
<ServiceFrame 
  xmlns="http://www.netex.org.uk/netex"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  version="1.11:NO-NeTEx-networktimetable:1.5">
    
    <destinationDisplays>
        
        
        <!-- Metro Line 5 to Sognsvann -->
        <DestinationDisplay id="RUT:DestinationDisplay:1" version="1">
            <SideText>5 Sognsvann</SideText>
            <FrontText>5 Sognsvann</FrontText>
            <PublicCode>5</PublicCode>
            <vias>
                <Via order="1">
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:VH" version="1"/>
                    <ViaType>stopPoint</ViaType>
                </Via>
                <Via order="2">
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:VR" version="1"/>
                    <ViaType>name</ViaType>
                </Via>
                <Via order="3">
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:VM" version="1"/>
                    <ViaType>stopPoint</ViaType>
                </Via>
            </vias>
            <variants>
                <DestinationDisplayVariant>
                    <DestinationDisplayVariantMediaType>web</DestinationDisplayVariantMediaType>
                    <FrontText>Sognsvann</FrontText>
                </DestinationDisplayVariant>
            </variants>
        </DestinationDisplay>
        
        
        <!-- Metro Line 5 to Vestli -->
        <DestinationDisplay id="RUT:DestinationDisplay:2" version="1">
            <SideText>5 Vestli</SideText>
            <FrontText>5 Vestli</FrontText>
            <PublicCode>5</PublicCode>
            <vias>
                <Via order="1">
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:VM" version="1"/>
                    <ViaType>stopPoint</ViaType>
                </Via>
                <Via order="2">
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:VR" version="1"/>
                    <ViaType>name</ViaType>
                </Via>
                <Via order="3">
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:VH" version="1"/>
                    <ViaType>stopPoint</ViaType>
                </Via>
            </vias>
            <variants>
                <DestinationDisplayVariant>
                    <DestinationDisplayVariantMediaType>web</DestinationDisplayVariantMediaType>
                    <FrontText>Vestli</FrontText>
                </DestinationDisplayVariant>
            </variants>
        </DestinationDisplay>
        
        
        <!-- Via -->
        <DestinationDisplay id="RUT:DestinationDisplay:VH" version="1">
            <FrontText>Hasle</FrontText>
        </DestinationDisplay>
        <DestinationDisplay id="RUT:DestinationDisplay:VR" version="1">
            <FrontText>Ringen</FrontText>
        </DestinationDisplay>
        <DestinationDisplay id="RUT:DestinationDisplay:VM" version="1">
            <FrontText>Majorstuen</FrontText>
        </DestinationDisplay>
    </destinationDisplays>
    
</ServiceFrame>