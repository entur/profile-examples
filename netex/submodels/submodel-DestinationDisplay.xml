<?xml version="1.0" encoding="UTF-8"?>
<!--
DestinationDisplay is used to specify text that is advertized to public, both on the screens on vehicle sides and on some other media
-->
<ServiceFrame 
  xmlns="http://www.netex.org.uk/netex"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  version="1.11:NO-NeTEx-networktimetable:1.4">
    
    <destinationDisplays>
        <!-- Metro Line 5 to Sognsvann -->
        <DestinationDisplay id="RUT:DestinationDisplay:1" version="1">
            <SideText>5 Sognsvann</SideText>
            <FrontText>5 Sognsvann</FrontText>
            <!-- PublicCode>5</PublicCode --> <!-- possible to have as separate value instead of embedded in text strings -->
            <vias>
                <Via>
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:3" version="1"/>
                    <ViaType>name</ViaType>
                </Via>
            </vias>
            <variants>
                <DestinationDisplayVariant>
                    <DestinationDisplayVariantMediaType>web</DestinationDisplayVariantMediaType>
                    <FrontText>Sognsvann</FrontText>
                </DestinationDisplayVariant>
            </variants>
        </DestinationDisplay>
        
        <!-- Metro Line 5 to Vestli -->
        <DestinationDisplay id="RUT:DestinationDisplay:2" version="1">
            <SideText>5 Vestli</SideText>
            <FrontText>5 Vestli</FrontText>
            <vias>
                <Via>
                    <DestinationDisplayRef ref="RUT:DestinationDisplay:3" version="1"/>
                    <ViaType>name</ViaType>
                </Via>
            </vias>
            <variants>
                <DestinationDisplayVariant>
                    <DestinationDisplayVariantMediaType>web</DestinationDisplayVariantMediaType>
                    <FrontText>Vestli</FrontText>
                </DestinationDisplayVariant>
            </variants>
        </DestinationDisplay>
        
        <!-- Via -->
        <DestinationDisplay id="RUT:DestinationDisplay:3" version="1">
            <FrontText>Ringen</FrontText>
        </DestinationDisplay>
    </destinationDisplays>
</ServiceFrame>