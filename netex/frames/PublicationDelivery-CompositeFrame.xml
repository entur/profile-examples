<?xml version="1.0" encoding="UTF-8"?>
<PublicationDelivery xmlns="http://www.netex.org.uk/netex"
  xmlns:siri="http://www.siri.org.uk/siri"
  xmlns:gml="http://www.opengis.net/gml/3.2"
  xmlns:core="http://www.govtalk.gov.uk/core"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:ifopt="http://www.ifopt.org.uk/ifopt"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <PublicationTimestamp>2016-12-22T12:00:00Z</PublicationTimestamp>
    <ParticipantRef>NSR</ParticipantRef>
    <Description>PublicationDelivery med CompositeFrame som setter ValidityCondition for -alle- underliggende frames</Description>

    <dataObjects>
        <!-- CompositeFrame is used to gather different Frames with the same ValidityCondition -->
        <CompositeFrame version="1" created="2016-12-22T12:00:00Z" id="NSR:CompositeFrame:ID">
            <!-- Validity span (period). Please note that this is implicitly applied to ALL frames in CompositeFrame -->
            <validityConditions>
                <AvailabilityCondition version="1" id="NSR:AvailabilityCondition:ID"/>
            </validityConditions>
            <frames>
                <ResourceFrame id="NSR:ResourceFrame:ID" version="1"/>                
                <SiteFrame id="NSR:SiteFrame:ID" version="1"/>
                <ServiceFrame id="NSR:ServiceFrame:ID" version="1"/>
                <ServiceCalendarFrame id="NSR:ServiceCalendarFrame:ID" version="1"/>
                <TimetableFrame id="NSR:TimetableFrame:ID" version="1"/>
            </frames>
        </CompositeFrame>
    </dataObjects>
</PublicationDelivery>
