							<ScopeOfTicketsAvailable/>
						</TicketingEquipment>
					</placeEquipments>
					<TransportMode>rail</TransportMode>
					<StopPlaceType>railStation</StopPlaceType>
					<Weighting>preferredInterchange</Weighting>
